{
  "serviceCore": {
    "daoFile": "config/dao.config",
    "servicesFile": "Components.xml"
  },
  "Caches": {
    "ModelLoginResult": {
      "CacheID": "ModelLoginResult",
      "Type": "Redis",
      "Host": "127.0.0.1",
      "Port": 6379,
      "lifeTime": {
        "hours": 12,
        "min": 0,
        "seconds": 0
      }
    }
  },
  "scriptsPath": "/usr/local/package/model-content/scripts",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
